    "history": "^4.10.1",
    "http-proxy-middleware": "^1.0.3",
    "i18next": "^19.3.2",
    "i18next-browser-languagedetector": "^4.0.2",
    "i18next-http-backend": "^1.0.6",
    "invariant": "^2.2.4",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "leaflet": "^1.6.0",
    "lodash": "^4.17.15",
    "netlify-cli": "^2.48.0",
    "notistack": "^0.9.17",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-google-login": "^5.1.10",
    "react-helmet": "^5.2.1",
    "react-i18next": "^11.3.3",
    "react-leaflet": "^2.6.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-injectors": "^1.3.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "serialize-javascript": "^3.0.0",
    "styled-components": "^5.0.1",
    "typeface-roboto": "^0.0.75",
    "typesafe-actions": "^5.1.0",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "npm-run-all --parallel build:**",
    "build:app": "react-scripts build",
    "build:lambda": "netlify-lambda build functions",
    "serve:lambda": "netlify-lambda serve functions",
    "eject": "react-scripts eject",
    "test": "react-scripts test",
    "lint:staged": "lint-staged",
    "typecheck": "tsc",
    "lint": "npm run typecheck && npm run lint:css && npm run lint:eslint",
    "lint:css": "stylelint '**/*.{js,ts,jsx,tsx}'",
    "lint:eslint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:eslint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "prettify": "prettier --write '**/*.{ts,tsx,jsx,js,json,md,html}'",
    "trans:extract": "i18next --config .i18next-parser.config.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.{css,scss,md,json}": [
      "prettier --write",
      "git add --force"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:staged && npm run typecheck"
    }
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.51",
    "@types/cookie": "^0.3.3",
    "@types/i18next": "^13.0.0",
    "@types/invariant": "^2.2.31",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-helmet": "^5.0.15",
    "@types/react-leaflet": "^2.5.1",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-logger": "^3.0.7",
    "@types/serialize-javascript": "^1.5.0",
    "@types/styled-components": "^5.0.1",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.22.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "eslint-plugin-redux-saga": "^1.1.3",
    "husky": "^4.2.3",
    "i18next-parser": "^1.0.2",
    "lint-staged": "^10.0.8",
    "netlify-lambda": "^1.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "stylelint": "^13.2.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^26.1.1"
  }
}
